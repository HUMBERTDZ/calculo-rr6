	/**ENVIA VALORES DE  BASE_LAYOUT_RR7 A CMBG***/
	@Modifying
	@Transactional
	@Query(value = "update cm set cm.valores = "
			+ " IIF((select SUM(MONT_MON_CONSOLIDADA) from BASE_LAYOUT_RR7 bl where  "
			+ " CONCAT(bl.CTA_QTO_NIV_NVO_CAT,bl.TIPO_MONEDA)  "
			+ " like CONCAT(cm.NIVEL1,cm.NIVEL2,cm.NIVEL3,cm.NIVEL4,cm.CVEMONEDA))IS NULL, 0, "
			+ " (select SUM(MONT_MON_CONSOLIDADA) from BASE_LAYOUT_RR7 bl where  "
			+ " CONCAT(bl.CTA_QTO_NIV_NVO_CAT,bl.TIPO_MONEDA)  "
			+ " like CONCAT(cm.NIVEL1,cm.NIVEL2,cm.NIVEL3,cm.NIVEL4,cm.CVEMONEDA))) "
			+ " from CMBG cm INNER join  BASE_LAYOUT_RR7 bl on CONCAT(bl.CTA_QTO_NIV_NVO_CAT,bl.TIPO_MONEDA) =  "
			+ " CONCAT(cm.NIVEL1,cm.NIVEL2,cm.NIVEL3,cm.NIVEL4,cm.CVEMONEDA) "
			+ " where bl.ANIO = :anio AND BL.MES = :mes ", nativeQuery = true)
	void copiarInfo(@Param("anio") int anio,@Param("mes") int mes);

	


